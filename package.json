		"start": "run-p start:**",
		"start:app": "webpack-dev-server --mode development --progress --color --hot --inline",
		"build": "run-p build:**",
		"build:app": "webpack -p --progress --colors && node -p 'process.env'",
		"build:app:serve": "cd dist && ws --spa index.html",
		"lint": "npm run tslint && npm run stylelint",
		"lint-fix": "npm run tslint-fix && npm run stylelint-fix",
		"tslint": "tslint '{src,public}/**/*.{ts,tsx,js,jsx}'",
		"tslint-fix": "tslint '{src,public}/**/*.{ts,tsx,js,jsx}' --fix",
		"stylelint": "stylelint '{src,public}/**/*.(css|scss|sass)'",
		"stylelint-fix": "stylelint '{src,public}/**/*.(css|scss|sass)' --fix",
		"prettier": "prettier --write '{src,public}/**/*.{ts,tsx,js,jsx,css,scss,sass,htm,html,json}' '*.{ts,tsx,js,jsx,css,scss,sass,htm,html,json}'",
		"format": "npm run prettier && npm run lint-fix",
		"analyze": "source-map-explorer 'dist/*.js'",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:snapshot": "jest --updateSnapshot",
		"commit": "git cz"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
